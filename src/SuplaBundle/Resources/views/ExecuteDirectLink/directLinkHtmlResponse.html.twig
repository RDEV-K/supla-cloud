{% extends 'SuplaBundle::vue-template.html.twig' %}

{% block beforeVue %}
    <script>
        window.component = {
            name: 'directLinkExecutionResult',
            props: {
                {% if failureReason %}failureReason: "{{ failureReason.value }}",{% endif %}
                action: "{{ action.name is defined ? action.name : action }}",
                directLink: {{ directLink | json_encode | raw }}
            }
        };
    </script>
{% endblock %}
